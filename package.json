{
  "name": "nextjs-turborepo",
  "private": true,
  "version": "3.1.0",
  "author": {
    "name": "Damian Nowakowski",
    "email": "silon.damian@gmail.com"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --help",
    "lint": "turbo lint --no-cache",
    "preview": "turbo preview",
    "generate": "turbo gen",
    "analyze": "ANALYZE=true turbo analyze",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "commit": "git-cz",
    "eslint-config-inspector": "npx @eslint/config-inspector"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/cz-commitlint": "19.2.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "commitizen": "4.3.0",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "inquirer": "9.2.16",
    "lint-staged": "15.2.2",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.5",
    "turbo": "2.0.14",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": "^20"
  }
}
