{
  "name": "nextjs-turborepo",
  "private": true,
  "version": "3.1.0",
  "author": {
    "name": "Damian Nowakowski",
    "email": "silon.damian@gmail.com"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --help",
    "lint": "turbo lint --no-cache",
    "preview": "turbo preview",
    "generate": "turbo gen",
    "analyze": "ANALYZE=true turbo analyze",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "commit": "git-cz",
    "eslint-config-inspector": "npx @eslint/config-inspector"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "catalog:",
    "@commitlint/config-conventional": "catalog:",
    "@commitlint/cz-commitlint": "catalog:",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "commitizen": "catalog:",
    "eslint": "catalog:",
    "husky": "catalog:",
    "inquirer": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": "^20"
  }
}
