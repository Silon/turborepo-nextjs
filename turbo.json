{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["web#sitemap"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "web#dev": {
      "dependsOn": ["web#generate-env-types"]
    },
    "web#sitemap": {
      "dependsOn": ["web#build"]
    },
    "web#build": {
      "dependsOn": ["web#lint"]
    },
    "web#lint": {
      "dependsOn": ["web#lint:eslint", "web#lint:typescript"]
    },
    "web#lint:eslint": {
      "dependsOn": ["web#generate-env-types"]
    },
    "web#lint:typescript": {
      "dependsOn": ["web#generate-env-types"]
    },
    "web#preview": {
      "dependsOn": ["web#start"]
    },
    "web#start": {
      "dependsOn": ["web#build"]
    },
    "web#analyze": {
      "dependsOn": ["web#build"]
    },
    "web#generate-env-types": {}
  }
}
